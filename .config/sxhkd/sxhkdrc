# Programs
alt + Return
	$TERMINAL

alt + p
	dmenu_run

alt + shift + {f,o,b}
	{firefox, firefox --new-window overleaf.com/login, firefox brightspace.ru.nl}

# Computer state
alt + l ; {shift + s, shift + r, l}
	{shutdown now, reboot, i3lock -fi ~/.config/wall.png}

# Keys with 'fn'
Print
	deepin-screenshot

XF86TouchpadToggle
	touchpad-toggle

XF86AudioMute 
	mute-toggle

XF86Audio{Lower,Raise}Volume
	pactl set-sink-volume 0 {-,+}2%
