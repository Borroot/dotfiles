# Programs
super + o ; {f, o}
	{firefox, firefox --new-window overleaf.com}

# Computer state
super + b ; {shift + s, shift + b, l}
	{shutdown now, reboot, i3lock -fi ~/.config/wall.png}

# Keys with 'fn'
Print
	deepin-screenshot

XF86TouchpadToggle
	touchpad-toggle

XF86Audio{Lower,Raise}Volume
	pactl set-sink-volume 0 {-,+}2%

XF86AudioMute 
	mute-toggle
