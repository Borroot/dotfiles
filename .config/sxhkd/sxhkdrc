# Programs
alt + Return
	$TERMINAL

alt + p
	dmenu_run

alt + n ; {v,d}
	{pcmanfm, discord}

alt + n ; {f,o,b,n,w}
	{firefox, firefox --new-window overleaf.com/login, firefox brightspace.ru.nl, firefox netflix.com/login, firefox --new-window web.whatsapp.com}

# Computer state
alt + y ; {shift + s, shift + r, l}
	{shutdown now, reboot, i3lock -fi ~/.config/wall.png}

# Keys with 'fn'
Print
	deepin-screenshot

XF86TouchpadToggle
	touchpad-toggle

XF86AudioMute 
	mute-toggle

XF86Audio{Lower,Raise}Volume
	pactl set-sink-volume 0 {-,+}2%
