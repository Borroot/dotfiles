# Programs
alt + Return
	$TERMINAL

alt + p
	dmenu_run

alt + n ; {v,d,s,p,e,a}
	{pcmanfm, discord, spotify, zathura, emacs, android-studio}

alt + n ; {f,b,w,g}
	qutebrowser { , brightspace.ru.nl, web.whatsapp.com --target window, github.com/borroot/\?tab=repositories}

alt + n ; {o,n}
	firefox {overleaf.com/login, netflix.com/login}

# Computer state
alt + y ; {shift + s, shift + r, l}
	{shutdown now, reboot, lock}

# Keys with 'fn'
Print
	deepin-screenshot

XF86TouchpadToggle
	touchpad-toggle

XF86AudioMute
	mute-toggle

XF86Audio{Lower,Raise}Volume
	pactl set-sink-volume 0 {-,+}2%

XF86MonBrightness{Down,Up}
	backlight {-,+}5
